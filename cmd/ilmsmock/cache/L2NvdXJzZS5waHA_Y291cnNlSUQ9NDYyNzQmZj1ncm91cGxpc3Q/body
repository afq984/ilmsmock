    function onFinish()
    {
        window.location.reload();
    }
    function add()
    {
        $showModal('重新分組', '/course/group/insert.php?courseID=46274', 750, 370, onFinish);
    }
    function addHW()
    {
        $showModal('新增作業', '/course/hw_insert.php?courseID=46274&groupID=30698', 785, 530, onFinish);
    }
    function editHW(id)
    {
        $showModal('編輯', '/course/hw_edit.php?id=' + id + '&groupID=30698' , 785, 480, onFinish);
    }

    function delHW(id)
    {
        $showModal('刪除', '/course/hw_delete.php?id=' + id, 350, 200, onFinish);
    }
    function addTeam()
    {
        $showModal('新增組別', '/course/group/team_insert.php?groupID=30698', 750, 200, onFinish);
    }
    function editTeam(teamID)
    {
        $showModal('編輯', '/course/group/team_edit.php?teamID=' + teamID, 750, 360, onFinish);
    }
    function delTeam(teamID)
    {
        $showModal('刪除分組', '/course/group/team_delete.php?teamID=' + teamID, 380, 280, onFinish);
    }
    function joinTeam(teamID)
    {
        $showModal('加入分組', '/course/group/team_join.php?teamID=' + teamID, 380, 280, onFinish);
    }
    function editGroup()
    {
        $showModal('設定', '/course/group/edit.php?groupID=30698', 750, 360, onFinish);
    }
